SELECT Rundentab.RT_ID, [Startklasse_text] & " - " & [Rundentext] AS Rundenname, Rundentab.Turniernr, Rundentab.Startklasse, Startklasse.Startklasse_text, Tanz_Runden_fix.Rundentext, Tanz_Runden_fix.Runde, Tanz_Runden_fix.R_IS_ENDRUNDE, Tanz_Runden_fix.R_NAME_ABLAUF, Tanz_Runden_fix.InRundeneinteilung, Tanz_Runden_fix.InAuswertung, Tanz_Runden_fix.InPunkteeingabe, Tanz_Runden_fix.MitStartklasse, Tanz_Runden_fix.R_IS_ENDRUNDE, Startklasse.Reihenfolge, Tanz_Runden_fix.Rundenreihenfolge, Rundentab.Rundenreihenfolge AS RF FROM Tanz_Runden_fix INNER JOIN (Startklasse INNER JOIN Rundentab ON Startklasse.Startklasse = Rundentab.Startklasse) ON Tanz_Runden_fix.Runde = Rundentab.Runde WHERE (((Rundentab.Turniernr)=[Formulare]![A-Programmübersicht]![akt_Turnier]) AND ((Rundentab.Rundenreihenfolge)<999)) OR (((Rundentab.Turniernr)=[Formulare]![A-Programmübersicht]![akt_Turnier]) AND ((Tanz_Runden_fix.Runde) Like "*_r_Fuß")) ORDER BY Startklasse.Reihenfolge, Tanz_Runden_fix.Rundenreihenfolge;
