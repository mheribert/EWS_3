SELECT DISTINCTROW Wert_Richter.WR_Kuerzel, geteilte_Endrunde.platz_langsam, geteilte_Endrunde.platz_schnell, geteilte_Endrunde.platz_sum, geteilte_Endrunde.gesamt_platz, View_Paare.Platz, Turnier.Turnier_Name, Turnier.T_Datum, Turnier.Turnier_Name, Turnier.Veranst_Name, Turnier.Turnier_Nummer, Turnier.Veranst_Ort, View_Paare.Name, View_Paare.Verein_Name, View_Paare.Startnr, geteilte_Endrunde.platz_sum, View_Runden.Name_Ablauf, View_Runden.R_NAME_ABLAUF, View_Runden.Startklasse_text, Wert_Richter.WR_Vorname, Wert_Richter.WR_Nachname, geteilte_Endrunde.RT_ID
FROM Wert_Richter INNER JOIN (((geteilte_Endrunde INNER JOIN View_Paare ON geteilte_Endrunde.TP_ID=View_Paare.TP_ID) INNER JOIN Turnier ON View_Paare.Turniernr=Turnier.Turniernum) INNER JOIN View_Runden ON View_Paare.RT_ID_Ausgeschieden=View_Runden.RT_ID) ON Wert_Richter.WR_ID=geteilte_Endrunde.WR_ID
GROUP BY Wert_Richter.WR_Kuerzel, geteilte_Endrunde.platz_langsam, geteilte_Endrunde.platz_schnell, geteilte_Endrunde.platz_sum, geteilte_Endrunde.gesamt_platz, View_Paare.Platz, Turnier.Turnier_Name, Turnier.T_Datum, Turnier.Turnier_Name, Turnier.Veranst_Name, Turnier.Turnier_Nummer, Turnier.Veranst_Ort, View_Paare.Name, View_Paare.Verein_Name, View_Paare.Startnr, geteilte_Endrunde.platz_sum, View_Runden.Name_Ablauf, View_Runden.R_NAME_ABLAUF, View_Runden.Startklasse_text, Wert_Richter.WR_Vorname, Wert_Richter.WR_Nachname, geteilte_Endrunde.RT_ID, Turnier.Turniernum, View_Paare.Platz
HAVING (((geteilte_Endrunde.RT_ID)=[Formulare]![A-Programmübersicht]![Report_RT_ID]))
ORDER BY Wert_Richter.WR_Kuerzel, geteilte_Endrunde.gesamt_platz, View_Paare.Platz;

