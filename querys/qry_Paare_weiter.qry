SELECT Paare_Rundenqualifikation.Startnummer AS Ausdr1, Paare_Rundenqualifikation.Startklass AS Ausdr2, Paare_Rundenqualifikation.Tanzrund AS Ausdr3, Paare_Rundenqualifikation.Turniernr AS Ausdr4, Turnier.Turnier_Name, Turnier.T_Datum, Paare.Da_Vorname, Paare.Da_NAchname, Paare.He_Vorname, Paare.He_Nachname, Paare.Verein_Name, Paare.Name_Team, Turnier.Turnier_Name, Turnier.Veranst_Name, Tanz_Runden.Rundentext, IIf([Name_Team] Is Not Null,[Name_Team],[Da_Vorname] & " " & [Da_NAchname] & " - " & [He_Vorname] & " " & [He_Nachname]) AS FoName, Startklasse.Startklasse_text
FROM Turnier, Tanz_Runden, Paare_Rundenqualifikation, Startklasse INNER JOIN Paare ON Startklasse.Startklasse=Paare.Startkl
GROUP BY Paare_Rundenqualifikation.Startnummer, Paare_Rundenqualifikation.Startklass, Paare_Rundenqualifikation.Tanzrund, Paare_Rundenqualifikation.Turniernr, Turnier.Turnier_Name, Turnier.T_Datum, Paare.Da_Vorname, Paare.Da_NAchname, Paare.He_Vorname, Paare.He_Nachname, Paare.Verein_Name, Paare.Name_Team, Turnier.Turnier_Name, Turnier.Veranst_Name, Tanz_Runden.Rundentext, IIf([Name_Team] Is Not Null,[Name_Team],[Da_Vorname] & " " & [Da_NAchname] & " - " & [He_Vorname] & " " & [He_Nachname]), Startklasse.Startklasse_text
HAVING (((Paare_Rundenqualifikation.Startklass)=Formulare![A-Programmübersicht]!ReportStartklasse) And ((Paare_Rundenqualifikation.Tanzrund)=Formulare![A-Programmübersicht]!ReportTanzrunde) And ((Paare_Rundenqualifikation.Turniernr)=Formulare![A-Programmübersicht]!akt_Turnier))
ORDER BY Paare_Rundenqualifikation.Startnummer;

