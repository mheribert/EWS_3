SELECT Paare.ausgeschieden AS Ausdr1, Paare.Platz, Majoritaet.Startnr AS Ausdr2, Majoritaet.Startkl AS Ausdr3, Majoritaet.Runde AS Ausdr4, Majoritaet.Turniernr AS Ausdr5, Sum(Majoritaet.Platz) AS [Summe von Platz], Turnier.Turnier_Name, Turnier.T_Datum, Paare.Da_Vorname, Paare.Da_NAchname, Paare.He_Vorname, Paare.He_Nachname, Paare.Verein_Name, Paare.Name_Team, Turnier.Turnier_Name, Turnier.Veranst_Name, Tanz_Runden.Rundentext, Turnier.BS_Name AS Ausdr1, Turnier.TL_Name AS Ausdr2, Turnier.AnsagerIn, Majoritaet.WR1, Majoritaet.WR2, Majoritaet.WR3, Majoritaet.WR4, Majoritaet.WR5, Majoritaet.WR6, Majoritaet.WR7, Startklasse.Startklasse_text, Paare.Punkte
FROM Turnier, Tanz_Runden, Majoritaet, Startklasse INNER JOIN Paare ON Startklasse.Startklasse=Paare.Startkl
GROUP BY Paare.ausgeschieden, Paare.Platz, Majoritaet.Startnr, Majoritaet.Startkl, Majoritaet.Runde, Majoritaet.Turniernr, Turnier.Turnier_Name, Turnier.T_Datum, Paare.Da_Vorname, Paare.Da_NAchname, Paare.He_Vorname, Paare.He_Nachname, Paare.Verein_Name, Paare.Name_Team, Turnier.Turnier_Name, Turnier.Veranst_Name, Tanz_Runden.Rundentext, Turnier.BS_Name, Turnier.TL_Name, Turnier.AnsagerIn, Majoritaet.WR1, Majoritaet.WR2, Majoritaet.WR3, Majoritaet.WR4, Majoritaet.WR5, Majoritaet.WR6, Majoritaet.WR7, Startklasse.Startklasse_text, Paare.Punkte
HAVING (((Paare.ausgeschieden)=Yes) And ((Majoritaet.Startkl)=Formulare!Ausdrucke![Startklasse einstellen]) And ((Majoritaet.Runde)=Formulare!Ausdrucke!Kombinationsfeld14) And ((Majoritaet.Turniernr)=Formulare![A-Programmübersicht]!akt_Turnier))
ORDER BY Sum(Majoritaet.Platz);

