SELECT runden.*, runden.Rundenreihenfolge, runden.InRundeneinteilung FROM Tanz_Runden INNER JOIN (Rundentab INNER JOIN runden ON Rundentab.Startklasse = runden.Startklasse) ON Tanz_Runden.Runde = Rundentab.Runde WHERE (((runden.Rundenreihenfolge)>[Tanz_runden].[Rundenreihenfolge]) AND ((Rundentab.RT_ID)=[Formulare]![Majoritaet_ausrechnen]![Startklasse]) AND ((runden.InRundeneinteilung)=1)) ORDER BY runden.Rundenreihenfolge;
