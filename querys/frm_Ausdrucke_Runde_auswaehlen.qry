SELECT Tanz_Runden.R_NAME_ABLAUF, Runden4Drucken.Startklasse_text, Runden4Drucken.RT_ID, Runden4Drucken.Runde, Runden4Drucken.Startklasse, Runden4Drucken.Turniernum, Runden4Drucken.Turnier_Name, Runden4Drucken.InRundeneinteilung, Startklasse.Reihenfolge, Tanz_Runden.Rundenreihenfolge FROM Tanz_Runden INNER JOIN (Startklasse INNER JOIN Runden4Drucken ON Startklasse.Startklasse = Runden4Drucken.Startklasse) ON Tanz_Runden.Runde = Runden4Drucken.Runde WHERE (((Runden4Drucken.InRundeneinteilung)>0)) ORDER BY Startklasse.Reihenfolge, Tanz_Runden.Rundenreihenfolge;
