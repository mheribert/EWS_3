SELECT DISTINCTROW View_Paare.Startklasse_text, View_Paare.Platz, View_Paare.Name, View_Paare.Verein_nr, View_Paare.Verein_Name, Turnier.Turnier_Name, Turnier.T_Datum, Turnier.Veranst_Name, View_Paare.Da_Vorname, View_Paare.Da_Nachname, View_Paare.He_Vorname, View_Paare.He_Nachname, View_Paare.Name_Team, View_Paare.Punkte, View_Paare.Startbuch, TLP_RR_PAARE.LRRVERB AS LV_RR, View_Paare.Boogie_Startkarte_H, TLP_BW_PAARE_1.LRRVERB AS LV_H, View_Paare.Boogie_Startkarte_D, TLP_BW_PAARE.LRRVERB AS LV_D, View_Paare.Turniernr
FROM TLP_BW_PAARE RIGHT JOIN (TLP_BW_PAARE AS TLP_BW_PAARE_1 RIGHT JOIN (TLP_RR_PAARE RIGHT JOIN ((Turnier INNER JOIN View_Paare ON Turnier.Turniernum = View_Paare.Turniernr) INNER JOIN Startklasse ON View_Paare.Startkl = Startklasse.Startklasse) ON TLP_RR_PAARE.Buchnr = View_Paare.Startbuch) ON TLP_BW_PAARE_1.Buchnr = View_Paare.Boogie_Startkarte_H) ON TLP_BW_PAARE.Buchnr = View_Paare.Boogie_Startkarte_D
WHERE (((View_Paare.Anwesent_Status)=1))
GROUP BY View_Paare.Startklasse_text, View_Paare.Platz, View_Paare.Name, View_Paare.Verein_nr, View_Paare.Verein_Name, Turnier.Turnier_Name, Turnier.T_Datum, Turnier.Veranst_Name, View_Paare.Da_Vorname, View_Paare.Da_Nachname, View_Paare.He_Vorname, View_Paare.He_Nachname, View_Paare.Name_Team, View_Paare.Punkte, View_Paare.Startbuch, TLP_RR_PAARE.LRRVERB, View_Paare.Boogie_Startkarte_H, TLP_BW_PAARE_1.LRRVERB, View_Paare.Boogie_Startkarte_D, TLP_BW_PAARE.LRRVERB, View_Paare.Turniernr, Startklasse.Reihenfolge, View_Paare.Platz
HAVING (((View_Paare.Platz)<>0))
ORDER BY Startklasse.Reihenfolge, View_Paare.Platz;

