SELECT StartklasseBeiTurnier.Startklasse, StartklasseBeiTurnier.Startklasse_text, StartklasseBeiTurnier.Reihenfolge, StartklasseBeiTurnier.isStartklasse, StartklasseBeiTurnier.Turniernr FROM StartklasseBeiTurnier WHERE (((StartklasseBeiTurnier.isStartklasse)=True) AND ((StartklasseBeiTurnier.Turniernr)=[Formulare]![A-Programmübersicht]![akt_Turnier])) ORDER BY StartklasseBeiTurnier.Reihenfolge;
