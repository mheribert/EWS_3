SELECT DISTINCT Startklasse.Startklasse, Startklasse.Startklasse_text, Startklasse.Reihenfolge FROM (Startklasse INNER JOIN Paare ON Startklasse.Startklasse = Paare.Startkl) INNER JOIN Startklasse_Turnier ON Startklasse.Startklasse = Startklasse_Turnier.Startklasse WHERE (((Startklasse.isStartklasse)=True) AND ((Startklasse.Anzahl_Startbuecher)=2) AND ((Startklasse_Turnier.Turniernr)=[Formulare]![A-Programmübersicht]![akt_Turnier])) ORDER BY Startklasse.Reihenfolge, Startklasse.Startklasse;
