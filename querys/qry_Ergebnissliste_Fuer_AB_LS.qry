SELECT DISTINCTROW Wert_Richter.WR_Kuerzel, View_Paare.Platz, Turnier.Turnier_Name, Turnier.T_Datum, Turnier.Veranst_Name, Turnier.Veranst_Ort, View_Paare.Name, View_Paare.Verein_Name, View_Paare.Startnr, View_Runden.Name_Ablauf, View_Runden.R_NAME_ABLAUF, View_Runden.Startklasse_text, Wert_Richter.WR_Vorname, Wert_Richter.WR_Nachname, geteiltes_Endergebnis.Punkte_FT, geteiltes_Endergebnis.Platz_FT, geteiltes_Endergebnis.Platz_FT, geteiltes_Endergebnis.Punkte_AK, geteiltes_Endergebnis.Platz_AK, geteiltes_Endergebnis.Platz_Summe, geteiltes_Endergebnis.Gesamt_Platz, View_Paare.Platz, IIf([DQ_ID]=1,"disqualifiziert") AS dq, geteiltes_Endergebnis.RegelV_FT, geteiltes_Endergebnis.RegelV_AK
FROM Wert_Richter INNER JOIN (geteiltes_Endergebnis INNER JOIN ((View_Paare INNER JOIN Turnier ON View_Paare.Turniernr=Turnier.Turniernum) INNER JOIN View_Runden ON View_Paare.RT_ID_Ausgeschieden=View_Runden.RT_ID) ON geteiltes_Endergebnis.TP_ID=View_Paare.TP_ID) ON Wert_Richter.WR_ID=geteiltes_Endergebnis.WR_ID
GROUP BY Wert_Richter.WR_Kuerzel, View_Paare.Platz, Turnier.Turnier_Name, Turnier.T_Datum, Turnier.Veranst_Name, Turnier.Veranst_Ort, View_Paare.Name, View_Paare.Verein_Name, View_Paare.Startnr, View_Runden.Name_Ablauf, View_Runden.R_NAME_ABLAUF, View_Runden.Startklasse_text, Wert_Richter.WR_Vorname, Wert_Richter.WR_Nachname, geteiltes_Endergebnis.Punkte_FT, geteiltes_Endergebnis.Platz_FT, geteiltes_Endergebnis.Platz_FT, geteiltes_Endergebnis.Punkte_AK, geteiltes_Endergebnis.Platz_AK, geteiltes_Endergebnis.Platz_Summe, geteiltes_Endergebnis.Gesamt_Platz, View_Paare.Platz, IIf([DQ_ID]=1,"disqualifiziert"), Wert_Richter.Turniernr, geteiltes_Endergebnis.RT_ID, geteiltes_Endergebnis.RegelV_FT, geteiltes_Endergebnis.RegelV_AK
HAVING (((Wert_Richter.Turniernr)=Formulare![A-Programmübersicht]!Akt_Turnier) And ((geteiltes_Endergebnis.RT_ID)=Formulare!Ausdrucke!RundenId))
ORDER BY Wert_Richter.WR_Kuerzel, View_Paare.Platz;

