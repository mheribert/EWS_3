SELECT Paare.Startkl, Paare.Startnr, Rundentab.Rundenreihenfolge, Rundentab.Runde, Paare.Da_Nachname, Paare.He_Nachname, Paare.Name_Team, IIf([Runde] Like "*Vor*",[Akro1_VR],IIf([Runde] Like "*ZW*",[Akro1_ZR],IIf([Runde] Like "*End*",[Akro1_ER],"nix"))) AS Akro1, IIf([Runde] Like "*Vor*",[Akro2_VR],IIf([Runde] Like "*ZW*",[Akro2_ZR],IIf([Runde] Like "*End*",[Akro2_ER],"nix"))) AS Akro2, IIf([Runde] Like "*Vor*",[Akro3_VR],IIf([Runde] Like "*ZW*",[Akro3_ZR],IIf([Runde] Like "*End*",[Akro3_ER],"nix"))) AS Akro3, IIf([Runde] Like "*Vor*",[Akro4_VR],IIf([Runde] Like "*ZW*",[Akro4_ZR],IIf([Runde] Like "*End*",[Akro4_ER],"nix"))) AS Akro4, IIf([Runde] Like "*Vor*",[Akro5_VR],IIf([Runde] Like "*ZW*",[Akro5_ZR],IIf([Runde] Like "*End*",[Akro5_ER],"nix"))) AS Akro5, IIf([Runde] Like "*Vor*",[Akro6_VR],IIf([Runde] Like "*ZW*",[Akro6_ZR],IIf([Runde] Like "*End*",[Akro6_ER],"nix"))) AS Akro6, IIf([Runde] Like "*Vor*",[Akro7_VR],IIf([Runde] Like "*ZW*",[Akro7_ZR],IIf([Runde] Like "*End*",[Akro7_ER],"nix"))) AS Akro7, IIf([Runde] Like "*Vor*",[Akro8_VR],IIf([Runde] Like "*ZW*",[Akro8_ZR],IIf([Runde] Like "*End*",[Akro8_ER],"nix"))) AS Akro8, IIf([Runde] Like "*Vor*",[E_Akro1_VR],IIf([Runde] Like "*ZW*",[E_Akro1_ZR],IIf([Runde] Like "*End*",[E_Akro1_ER],"nix"))) AS E_Akro1, IIf([Runde] Like "*Vor*",[E_Akro2_VR],IIf([Runde] Like "*ZW*",[E_Akro2_ZR],IIf([Runde] Like "*End*",[E_Akro2_ER],"nix"))) AS E_Akro2, Paare.Musik_FT, Paare.Musik_Akro, Paare.Musik_Stell, Paare.Musik_Form, Paare.Musik_Sieg, Paare.Anz_Taenzer
FROM Paare INNER JOIN Rundentab ON Paare.Startkl = Rundentab.Startklasse
WHERE (((Paare.Startkl) Like "RR_A" Or (Paare.Startkl) Like "RR_B" Or (Paare.Startkl) Like "RR_C" Or (Paare.Startkl) Like "RR_J" Or (Paare.Startkl) Like "*F_RR_M" Or (Paare.Startkl) Like "*F_RR_LF" Or (Paare.Startkl) Like "*F_RR_GF" Or (Paare.Startkl) Like "*F_RR_ST" Or (Paare.Startkl) Like "*F_RR_J" Or (Paare.Startkl) Like "*F_BW_M") AND ((Rundentab.Runde) Not Like "*Fuﬂ*" And ((Rundentab.Runde) Like "*Vor*" Or (Rundentab.Runde) Like "*Zw*" Or (Rundentab.Runde) Like "*end*")))
ORDER BY Paare.Startkl, Paare.Startnr, Rundentab.Rundenreihenfolge;

