SELECT [AAAA_Auswertung Wertungen].Startklasse, [AAAA_Auswertung Wertungen].Runde, [AAAA_Auswertung Wertungen].Startbuch, [AAAA_Auswertung Wertungen].Paar, [AAAA_Auswertung Wertungen].Startnr, 100-10*[Herr_Grundtechnik] AS Grundtechnik_Herr, 100-10*[Herr_Haltung_Drehtechnik] AS Haltung_Drehtechnik_Herr, 100-10*[Dame_Grundtechnik] AS Grundtechnik_Dame, 100-10*[Dame_Haltung_Drehtechnik] AS Haltung_Drehtechnik_Dame, 100-10*[Choreographie] AS Tanz_Choreographie, 100-10*[Tanzfiguren] AS Tanz_Tanzfiguren, 100-10*[Tänzerische_Darbietung] AS Tanz_Tänzerische_Darbietung, [AAAA_Auswertung Wertungen].Grobfehler_Text, [AAAA_Auswertung Wertungen].Grobfehler_Summe, Round((100-[Akrobatik1]*100/[Akro1Punkte])/5,0)*5 AS Akrobatik_1, Round((100-[Akrobatik2]*100/[Akro2Punkte])/5,0)*5 AS Akrobatik_2, Round((100-[Akrobatik3]*100/[Akro3Punkte])/5,0)*5 AS Akrobatik_3, Round((100-[Akrobatik4]*100/[Akro4Punkte])/5,0)*5 AS Akrobatik_4, Round((100-[Akrobatik5]*100/[Akro5Punkte])/5,0)*5 AS Akrobatik_5, Round(100-[Akrobatik6]*100/[Akro6Punkte],0) AS Akrobatik_6, 0 AS Gesamtpunkte, [AAAA_Auswertung Wertungen].Platz, [AAAA_Auswertung Wertungen].Punkte AS Ranglistenpunkte
FROM [AAAA_Auswertung Wertungen]
WHERE ((([AAAA_Auswertung Wertungen].WR_Azubi)=No));

