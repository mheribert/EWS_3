SELECT IIf(View_Majoritaet.DQ_ID>0,1,0) AS disqualifiziert, View_Majoritaet.Runde_Report, View_Majoritaet.Platz, View_Paare.Startnr, View_Paare.Startkl, View_Runden.Runde, View_Paare.Turniernr, View_Paare.Name, View_Paare.Da_Vorname, View_Paare.Da_Nachname, View_Paare.He_Vorname, View_Paare.He_Nachname, View_Paare.Verein_Name, View_Paare.Name_Team, IIf(Tanz_Runden.R_IS_ENDRUNDE=1,paare!Punkte," ") AS Punkte_anz, Turnier.Turnier_Name, Turnier.Turnier_Nummer, Turnier.T_Datum, View_Paare.Startklasse_text, Turnier.Veranst_Name, Tanz_Runden.Rundentext, Turnier.Veranst_Ort, View_Majoritaet.Majoritaet, View_Majoritaet.RT_ID, Tanz_Runden.R_NAME_ABLAUF, View_Paare.Punkte, View_Majoritaet.DQ_Grund, View_Majoritaet.Anmerkung
FROM ((Turnier INNER JOIN View_Paare ON Turnier.Turniernum=View_Paare.Turniernr) INNER JOIN View_Majoritaet ON View_Paare.TP_ID=View_Majoritaet.TP_ID) INNER JOIN (Tanz_Runden INNER JOIN View_Runden ON Tanz_Runden.Runde=View_Runden.Runde) ON View_Majoritaet.RT_ID=View_Runden.RT_ID
GROUP BY IIf(View_Majoritaet.DQ_ID>0,1,0), View_Majoritaet.Runde_Report, View_Majoritaet.Platz, View_Paare.Startnr, View_Paare.Startkl, View_Runden.Runde, View_Paare.Turniernr, View_Paare.Name, View_Paare.Da_Vorname, View_Paare.Da_Nachname, View_Paare.He_Vorname, View_Paare.He_Nachname, View_Paare.Verein_Name, View_Paare.Name_Team, IIf(Tanz_Runden.R_IS_ENDRUNDE=1,paare!Punkte," "), Turnier.Turnier_Name, Turnier.Turnier_Nummer, Turnier.T_Datum, View_Paare.Startklasse_text, Turnier.Veranst_Name, Tanz_Runden.Rundentext, Turnier.Veranst_Ort, View_Majoritaet.Majoritaet, View_Majoritaet.RT_ID, Tanz_Runden.R_NAME_ABLAUF, View_Paare.Punkte, View_Majoritaet.DQ_Grund, View_Majoritaet.Anmerkung
HAVING (((View_Majoritaet.RT_ID)=Formulare![A-Programmübersicht]!Report_RT_ID))
ORDER BY IIf(View_Majoritaet.DQ_ID>0,1,0), View_Majoritaet.Platz, View_Paare.Startnr;

