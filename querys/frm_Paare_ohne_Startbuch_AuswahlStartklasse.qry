SELECT DISTINCT Startklasse.Startklasse, Startklasse.Startklasse_text, Startklasse.Reihenfolge FROM (Startklasse INNER JOIN Paare ON Startklasse.Startklasse = Paare.Startkl) INNER JOIN Startklasse_Turnier ON Startklasse.Startklasse = Startklasse_Turnier.Startklasse WHERE (((Startklasse.isStartklasse)=True)) ORDER BY Startklasse.Reihenfolge, Startklasse.Startklasse;
