SELECT Paare.TP_ID, Paare.Turniernr, Paare.Startkl, Startklasse.Startklasse_text, Paare.Startnr, IIf([Name_Team]<>"",[Name_Team],[Da_Vorname] & " " & [Da_Nachname] & " - " & [He_Vorname] & " " & [He_Nachname]) AS Name, Paare.Verein_Name, Paare.Da_Vorname, Paare.Da_Nachname, Paare.He_Vorname, Paare.He_Nachname, Paare.Verein_nr, Paare.Name_Team, Paare.Platz, Paare.Punkte, Paare.RT_ID_Ausgeschieden, Paare.Runde_Report, Paare.Startbuch, Paare.Boogie_Startkarte_H, Paare.Boogie_Startkarte_D, Paare.Anwesent_Status, Paare.SBS_ID, Paare.SBS_ID_BW_D, Paare.SBS_ID_BW_H, Paare.Da_Alterskontrolle, Paare.He_Alterskontrolle, Paare.Akro1_VR, Paare.Wert1_VR, Paare.Akro2_VR, Paare.Wert2_VR, Paare.Akro3_VR, Paare.Wert3_VR, Paare.Akro4_VR, Paare.Wert4_VR, Paare.Akro5_VR, Paare.Wert5_VR, Paare.Akro6_VR, Paare.Wert6_VR, Paare.Akro7_VR, Paare.Wert7_VR, Paare.Akro8_VR, Paare.Wert8_VR, Paare.Akro1_ZR, Paare.Wert1_ZR, Paare.Akro2_ZR, Paare.Wert2_ZR, Paare.Akro3_ZR, Paare.Wert3_ZR, Paare.Akro4_ZR, Paare.Wert4_ZR, Paare.Akro5_ZR, Paare.Wert5_ZR, Paare.Akro6_ZR, Paare.Wert6_ZR, Paare.Akro7_ZR, Paare.Wert7_ZR, Paare.Akro8_ZR, Paare.Wert8_ZR, Paare.Akro1_ER, Paare.Wert1_ER, Paare.Akro2_ER, Paare.Wert2_ER, Paare.Akro3_ER, Paare.Wert3_ER, Paare.Akro4_ER, Paare.Wert4_ER, Paare.Akro5_ER, Paare.Wert5_ER, Paare.Akro6_ER, Paare.Wert6_ER, Paare.Akro7_ER, Paare.Wert7_ER, Paare.Akro8_ER, Paare.Wert8_ER, Paare.Anz_Taenzer
FROM Startklasse INNER JOIN Paare ON Startklasse.Startklasse = Paare.Startkl
ORDER BY Startklasse.Reihenfolge;

