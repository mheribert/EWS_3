SELECT DISTINCT Tanz_Runden_fix.Runde, Tanz_Runden_fix.R_NAME_ABLAUF AS Rundentext, Tanz_Runden_fix.Rundenreihenfolge, Tanz_Runden_fix.zweite_Runde, Rundentab.RT_ID FROM Tanz_Runden_fix INNER JOIN Rundentab ON Tanz_Runden_fix.Runde = Rundentab.Runde WHERE (((Tanz_Runden_fix.Rundenreihenfolge)<1000) AND ((Rundentab.Startklasse)=[Formulare]![Paare_in erste Runde nehmen]![Startklasse]) AND ((Rundentab.Turniernr)=[Formulare]![A-Programmübersicht]![akt_Turnier]) AND ((Tanz_Runden_fix.InRundeneinteilung)=1) AND ((Tanz_Runden_fix.InAuswertung)=1) AND ((Rundentab.Rundenreihenfolge)<1000)) ORDER BY Tanz_Runden_fix.Rundenreihenfolge;
