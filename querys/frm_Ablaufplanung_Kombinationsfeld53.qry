SELECT Tanz_Runden_fix.Runde, Tanz_Runden_fix.Rundentext, Tanz_Runden_fix.Rundenreihenfolge, Tanz_Runden_fix.MitStartklasse, Tanz_Runden_fix.R_IS_ENDRUNDE FROM Tanz_Runden_fix WHERE Tanz_Runden_fix.Runde NOT LIKE 'MK_*' UNION SELECT Runde, Rundentext, Rundenreihenfolge, MitStartklasse, R_IS_ENDRUNDE FROM Tanz_Runden_fix WHERE Runde Like 'MK_3*' Or Runde Like 'MK_4*' Or Runde Like 'MK_5*' UNION SELECT Runde, Rundentext, Rundenreihenfolge, MitStartklasse, R_IS_ENDRUNDE FROM Tanz_Runden_erg ORDER BY Rundenreihenfolge;
