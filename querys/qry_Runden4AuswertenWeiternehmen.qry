SELECT DISTINCT Rundentab.RT_ID, [Startklasse_text] & " - " & [Rundentext] AS Rundenname, Turnier.Turniernum, Rundentab.Startklasse, Startklasse.Startklasse_text, Tanz_Runden.Rundentext, Turnier.Turnier_Name, Tanz_Runden.Runde, Tanz_Runden.R_IS_ENDRUNDE, Startklasse_Turnier.AnzahlWR, Startklasse.Reihenfolge, Tanz_Runden.Rundenreihenfolge, Tanz_Runden.InAuswertung, Tanz_Runden.InRundeneinteilung, Rundentab.Rundenreihenfolge
FROM (Tanz_Runden INNER JOIN (Startklasse INNER JOIN ((Turnier INNER JOIN Rundentab ON Turnier.[Turniernum] = Rundentab.[Turniernr]) INNER JOIN Startklasse_Turnier ON Turnier.[Turniernum] = Startklasse_Turnier.[Turniernr]) ON (Startklasse.[Startklasse] = Rundentab.[Startklasse]) AND (Startklasse.[Startklasse] = Startklasse_Turnier.[Startklasse])) ON Tanz_Runden.[Runde] = Rundentab.[Runde]) INNER JOIN Paare_Rundenqualifikation ON Rundentab.[RT_ID] = Paare_Rundenqualifikation.[RT_ID]
WHERE (((Turnier.Turniernum)=[Formulare]![A-Programmübersicht]![akt_turnier]) AND ((Tanz_Runden.InAuswertung)=Yes)) OR (((Turnier.Turniernum)=[Formulare]![A-Programmübersicht]![akt_turnier]) AND ((Tanz_Runden.Runde)='End_r') AND ((Startklasse.Startklasse)='BW_H' Or (Startklasse.Startklasse)='BW_O' Or (Startklasse.Startklasse)='RR_A' Or (Startklasse.Startklasse)='RR_B'))
ORDER BY Startklasse.Reihenfolge, Tanz_Runden.Rundenreihenfolge;

